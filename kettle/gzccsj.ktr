    </partitioning>
    <connection>businessdata</connection>
    <sql>select t.v_date as l_date,
       t.n_zqsl as i_current_amount,
       t.n_zqcb + t.n_gzyzj + t.n_zqlx as en_marketvalue,
	   t.n_zqcb + t.n_zqlx as en_invest_balance,
	   t.n_zqcb as EN_MARKETVALUE_COST,
       t.n_zqcb/t.n_zqsl as en_cost_price,
       (t.n_zqcb + t.n_gzyzj + t.n_zqlx)/t.n_zqsl as en_gz_price,
       t.v_jjdm,
       t.v_zqdm,
       case
         when t.v_scdm = 'S' then
           'H'
         when t.v_scdm = 'Z' then
           'S'
         when t.v_scdm = 'Y' then
           'Z'
         when t.v_scdm = 'H' and t.v_kmbm not like '110265%' and t.v_kmbm not like '110266%' then
           'K'
         when t.v_scdm = 'H' and t.v_kmbm like '%110265%' then
           'HK'
         when t.v_scdm = 'H' and t.v_kmbm like '%110266%' then
           'SK'
         when t.v_scdm = 'J' then 
           'J'
         when t.v_scdm = 'O' then
           'Y'
         else 'Y'
         end as vc_exchange,
       t.v_kmbm,
       t.v_zcdl,
       to_char(t.d_timestamp,'yyyymmddhhmiss') as vc_compare
 from t_invest_port_detail t inner join 
 (select a.v_jjdm,max(a.v_date) as v_date from t_invest_port_detail a
   where a.v_zcdl not in (' ','C','DEP','R') group by a.v_jjdm) b
 on t.v_jjdm = b.v_jjdm  and t.v_date = b.v_date
 and t.v_date > ? and t.n_zqsl > 0</sql>
    <limit>0</limit>
    <lookup>估值持仓数据时间点输入</lookup>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
